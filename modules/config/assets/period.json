{
  "type": {
    "S": "period"
  },
  %{ if start_time != null ~}
  "begintime": {
    "S": "${ start_time }"
  },
  %{ endif ~}
  %{ if end_time != null ~}
  "endtime": {
    "S": "${ end_time }"
  },
  %{ endif ~}
  %{ if weekdays != null ~} 
  "weekdays": {
    "SS": ${ jsonencode(weekdays) }
  },
  %{ endif ~}
  %{ if months != null ~}}
  "months": {
    "SS": ${ jsonencode(months) }
  },
  %{ endif ~}
  "description": {
    "S": "${ description }"
  },
  "name": {
    "S": "${ name }"
  }
}
