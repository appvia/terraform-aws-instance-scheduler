{
  "type": {
    "S": "schedule"
  },
  %{ if periods != null ~}
  "periods": {
    "SS": ${ jsonencode(periods) }
  },
  %{ endif ~}
  %{ if override_status != null ~}
  "override_status": {
    "S": "${ override_status }"
  },
  %{ endif ~}
  %{ if timezone != null ~}
  "timezone": {
    "S": "${ timezone }"
  },
  %{ endif ~}
  %{ if enforced != null ~}
  "enforced": {
    "s": ${ "${ enforced }" }
  },
  %{ endif ~}
  %{ if stop_new_instances != null ~}
  "stop_new_instances": {
    "S": "${ stop_new_instances }" }
  },
  %{ endif ~}
  "description": {
    "S": "${ description }"
  },
  "name": {
    "S": "${ name }"
  }
}
